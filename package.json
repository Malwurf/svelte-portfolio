  "version": "1.0.0",
  "devDependencies": {
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-svelte": "^5.0.3",
